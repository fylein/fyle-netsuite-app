{
  "name": "fyle-netsuite-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "build": "npm run setup:env && ng build --prod --output-path=dist",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "setup:env": "node scripts/setup_env.js",
    "cypress:open": "cypress open -b chrome --e2e --env coverage=false",
    "cypress:run": "npm run remove_existing_code_coverage && cypress run -b chrome --env coverage=true",
    "setup_e2e_test": "npm run setup:env && node scripts/setup_e2e_test.js",
    "remove_existing_code_coverage": "rm -rf .nyc_output cypress/coverage",
    "cypress": "cypress",
    "stop": "kill $(lsof -t -i:4200) || true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~10.2.5",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.0.2",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@rxweb/reactive-form-validators": "~2.0.0",
    "cypress": "^12.0.1",
    "flatpickr": "^4.6.3",
    "i": "^0.3.6",
    "ng2-flatpickr": "^9.0.0",
    "ngx-cacheable": "^1.4.2",
    "normalize.css": "^8.0.1",
    "npm": "^6.13.7",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.4.1",
    "@angular-builders/dev-server": "^7.3.1",
    "@angular-devkit/build-angular": "^0.803.25",
    "@angular/cli": "~8.3.23",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@cypress/code-coverage": "^3.10.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.2.2",
    "dotenv": "^8.2.0",
    "prettier": "^1.19.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack": "^4.44.1",
    "webpack-dev-server": "^3.11.0"
  }
}
